{
  "name": "evc-app",
  "version": "1.0.0",
  "description": "EVC server",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "up": "node index.js",
    "start:prod": "npm run link-web && npm run build && TZ=utc node index.js",
    "start": "TZ=utc ts-node index.ts",
    "start:sse": "TZ=utc ts-node endpoints/index-price-sse.ts",
    "dev": "npm run clean && nodemon -w src --ext ts,yml --exec npm start",
    "dev:symbols": "TZ=utc ts-node -e 'import {start} from \"./endpoints/index-symbols\"; start()'",
    "build": "npm run lint && npm run clean && npm run compile",
    "build:prod": "rm -rf www && npm run compile",
    "clean": "ts-clean && rm -rf www",
    "compile": "tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "link-web": "rm -rf www && ln -sfn \"$(pwd)/../evc-web/build\" www",
    "migrate:up": "npm run typeorm migration:run",
    "migrate:down": "npm run typeorm migration:revert",
    "migrate:new": "npm run typeorm migration:create -- -n",
    "migrate:gen": "ts-node typeorm migration:generate -n",
    "typeorm": "TS_NODE_PROJECT=tsconfig.ts-node.json TS_NODE_TRANSPILE_ONLY=true ts-node ./node_modules/typeorm/cli.js"
  },
  "author": "techseeding2020@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "jest": "^26.6.3",
    "nodemon": "^2.0.4",
    "ts-clean": "^1.0.3",
    "ts-jest": "^26.4.4",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.6",
    "@types/node": "^14.0.5",
    "aws-sdk": "^2.686.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "delay": "^4.4.0",
    "dotenv": "^8.2.0",
    "email-templates": "^7.1.1",
    "error-to-json": "^2.0.0",
    "eventsource": "^1.0.7",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-fileupload": "^1.1.7-alpha.3",
    "express-list-endpoints": "^4.0.1",
    "express-sse-middleware": "^3.0.3",
    "geoip-lite": "^1.4.2",
    "handlebars": "^4.7.6",
    "html-to-text": "^6.0.0",
    "http-assert": "^1.4.1",
    "iexcloud_api_wrapper": "^1.1.5",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "md-to-pdf": "^3.1.1",
    "md5": "^2.3.0",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.32",
    "ms": "^2.1.3",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.4.14",
    "pg": "^8.3.3",
    "query-string": "^6.13.7",
    "redis": "^3.0.2",
    "rxjs": "^6.6.3",
    "serialize-error": "^7.0.1",
    "stripe": "^8.126.0",
    "swagger-routes-express": "^3.1.2",
    "typeorm": "^0.2.25",
    "typeorm-plus": "^0.1.2",
    "ua-parser-js": "^0.7.22",
    "uuid": "^8.3.0",
    "ws": "^7.3.1",
    "yamljs": "^0.3.0"
  }
}
