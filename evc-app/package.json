{
  "name": "evc-app",
  "version": "1.0.0",
  "description": "EVC server",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "up": "node index.js",
    "start:prod": "npm run link-web && npm run build && TZ=utc node index.js",
    "start": "TZ=utc ts-node index.ts",
    "daemon:price": "TZ=utc ts-node endpoints/index-price-sse.ts",
    "daemon:price:prod": "TZ=utc node endpoints/index-price-sse",
    "sync:schema": "TZ=utc ts-node endpoints/sync-db-schema.ts",
    "sync:schema:prod": "TZ=utc node endpoints/sync-db-schema",
    "dev": "npm run clean && nodemon -w src --ext ts,yml --exec npm start",
    "feed:symbols": "TZ=utc ts-node ./endpoints/feed-symbols.ts",
    "feed:symbols:prod": "TZ=utc node ./endpoints/feed-symbols",
    "feed:eps": "TZ=utc ts-node ./endpoints/feed-eps.ts",
    "feed:eps:prod": "TZ=utc node ./endpoints/feed-eps",
    "feed:pe": "TZ=utc ts-node ./endpoints/feed-close-pe.ts",
    "feed:pe:prod": "TZ=utc node ./endpoints/feed-close-pe",
    "daily:fetch": "TZ=utc ts-node ./endpoints/index-daily-fetch.ts",
    "daily:fetch:prod": "TZ=utc node ./endpoints/index-daily-fetch",
    "feed": "pnpm feed:symbols; pnpm feed:eps; pnpm feed:pe",
    "feed:prod": "pnpm feed:symbols:prod; pnpm feed:eps:prod; pnpm feed:pe:prod",
    "build": "npm run lint && npm run clean && npm run compile",
    "build:prod": "rm -rf www && npm run compile",
    "clean": "ts-clean && rm -rf www",
    "compile": "tsc",
    "lint": "eslint --fix -c .eslintrc.js --ext .ts  .",
    "link-web": "rm -rf www && ln -sfn \"$(pwd)/../evc-web/build\" www",
    "migrate:up": "npm run typeorm migration:run",
    "migrate:down": "npm run typeorm migration:revert",
    "migrate:new": "npm run typeorm migration:create -- -n",
    "migrate:gen": "ts-node typeorm migration:generate -n",
    "typeorm": "TS_NODE_PROJECT=tsconfig.ts-node.json TS_NODE_TRANSPILE_ONLY=true ts-node ./node_modules/typeorm/cli.js",
    "analyze": "pnpm build; source-map-explorer 'src/**/*.js'"
  },
  "author": "techseeding2020@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/eslint-plugin-tslint": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.20.0",
    "eslint-plugin-jsdoc": "^32.1.0",
    "eslint-plugin-no-null": "^1.0.2",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "ts-clean": "^1.0.3",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.31",
    "alphavantage": "^2.2.0",
    "aws-sdk": "^2.848.0",
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "csv": "^5.4.0",
    "csv-parse": "^4.15.3",
    "csv-parser": "^3.0.0",
    "delay": "^5.0.0",
    "dotenv": "^8.2.0",
    "error-to-json": "^2.0.0",
    "eventsource": "^1.0.7",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-fileupload": "^1.2.1",
    "express-list-endpoints": "^5.0.0",
    "express-sse-middleware": "^3.0.3",
    "geoip-lite": "^1.4.2",
    "handlebars": "^4.7.7",
    "html-to-text": "^7.0.0",
    "http-assert": "^1.4.1",
    "iexcloud_api_wrapper": "^1.1.5",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "ms": "^2.1.3",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.4.18",
    "pg": "^8.5.1",
    "query-string": "^6.14.0",
    "redis": "^3.0.2",
    "rxjs": "^6.6.3",
    "serialize-error": "^8.0.1",
    "serve-static": "^1.14.1",
    "sleep-promise": "^9.1.0",
    "stripe": "^8.137.0",
    "swagger-routes-express": "^3.3.0",
    "typeorm": "^0.2.31",
    "typeorm-plus": "^0.1.2",
    "ua-parser-js": "^0.7.24",
    "uuid": "^8.3.2",
    "ws": "^7.4.3",
    "yamljs": "^0.3.0"
  }
}
